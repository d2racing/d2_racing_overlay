		<separator/>
		<menu id="Configuration" label="Configuration">
			<menu id="Effects" label="Effects 3D">
				<item label="Pas effets">
					<action name="Execute">
						<execute>
							~/.config/openbox/scripts/xcompmgr.sh unset
						</execute>
					</action>
				</item>
				<item label="Transparent">
					<action name="Execute">
						<execute>
							~/.config/openbox/scripts/xcompmgr.sh set
						</execute>
					</action>
				</item>
				<item label="Transparent, fading">
					<action name="Execute">
						<execute>
							~/.config/openbox/scripts/xcompmgr.sh setshaded
						</execute>
					</action>
				</item>
				<item label="Transparent, fading shadows">
					<action name="Execute">
						<execute>
							~/.config/openbox/scripts/xcompmgr.sh setshadowshade
						</execute>
					</action>
				</item>
			</menu>
			<menu id="Conky" label="Config Conky">
				<item label="Edit .conkyrc">
					<action name="Execute">
						<execute>
							leafpad ~/.conkyrc
						</execute>
					</action>
				</item>
				<item label="Conky">
					<action name="Execute">
						<execute>
							conky &amp;
						</execute>
					</action>
				</item>
			</menu>
			<menu id="Obconfig" label="Openbox Config">
				<item label="Edit autostart">
