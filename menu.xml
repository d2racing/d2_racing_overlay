			</item>
			<menu id="Screenshots" label="Screenshot">
				<item label="Maintenant">
					<action name="Execute">
						<execute>
							scrot '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; geeqie ~/$f'
						</execute>
					</action>
				</item>
				<item label="Dans 5 Secondes...">
					<action name="Execute">
						<execute>
							scrot -d 5 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; geeqie ~/$f'
						</execute>
					</action>
				</item>
				<item label="Dans 10 Secondes...">
					<action name="Execute">
						<execute>
							scrot -d 10 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; geeqie ~/$f'
						</execute>
					</action>
				</item>
				<item label="Endroit precis... (click &amp; drag mouse)">
					<action name="Execute">
						<execute>
							scrot -s '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; geeqie ~/$f'
						</execute>
					</action>
				</item>
			</menu>
